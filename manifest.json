{
  "name": "Custom Styles",
  "description": "Inject custom CSS stuff",
  "version": "1.0",
  "content_scripts": [
    {
    "css": ["css/trello.css"],
    "matches": ["https://trello.com/*"]
    },
    {
    "css": ["css/3st.css"],
    "matches": ["https://www.3st.de/*"]
    },
    {
    "css": ["css/synfoni.css"],
    "js": ["js/synfoni.js"],
    "matches": ["https://*.synfoni.de/*"]
    },
    // {
    // "css": ["css/diro.css"],
    // "matches": ["http://*.3st-dev.de/*"]
    // },
    {
    "css": ["css/drupal.css"],
    "matches": ["https://*.de/admin/*", "http://*.de/admin/*", "https://*.com/admin/*", "http://*.com/admin/*"]
    },
    {
    "css": ["css/gitlab.css"],
    "js": ["js/gitlab.js"],
    "matches": ["https://gitlab.3st-dev.de/*"]
    }
  ],
  "manifest_version": 2
}